<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Background circle -->
    <item>
        <shape android:shape="oval">
            <solid android:color="#F5F5F5" />
        </shape>
    </item>
    
    <!-- Person icon -->
    <item
        android:width="32dp"
        android:height="32dp"
        android:gravity="center">
        <shape android:shape="oval">
            <solid android:color="#E0E0E0" />
        </shape>
    </item>
</layer-list> 